<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.htw.lambtenance.FXMLController">
    <children>
        <Button fx:id="startFactoryButton" layoutX="14.0" layoutY="14.0" onAction="#startFactory" text="Start factory" />
      <Button fx:id="stopFactoryButton" cancelButton="true" disable="true" layoutX="126.0" layoutY="14.0" onAction="#stopFactory" text="Stop factory" />
      <Button layoutX="161.0" layoutY="81.0" onAction="#generatePropertyError" text="Property Error" />
      <Button layoutX="14.0" layoutY="81.0" onAction="#generatePropertyAnomaly" text="Property Anomaly" />
      <Button layoutX="14.0" layoutY="116.0" onAction="#generateMachineAnomaly" text="Machine Anomaly" />
      <Button layoutX="160.0" layoutY="116.0" onAction="#generateMachineError" text="Machine Error" />
      <Button layoutX="195.0" layoutY="160.0" onAction="#repairFactory" text="Repair factory" />
    </children>
</AnchorPane>
